<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="maba_oauth_commerce_encrypted_credentials_session" pattern="/session">
        <default key="_controller">MabaOAuthCommerceEncryptedCredentialsBundle:Session:session</default>
        <requirement key="_method">POST</requirement>
    </route>
    <route id="maba_oauth_commerce_encrypted_credentials_token" pattern="/token">
        <default key="_controller">MabaOAuthCommerceEncryptedCredentialsBundle:Session:token</default>
        <requirement key="_method">POST</requirement>
    </route>
</routes>
