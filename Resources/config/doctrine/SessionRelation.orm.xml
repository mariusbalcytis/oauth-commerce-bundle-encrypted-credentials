<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Maba\Bundle\OAuthCommerceEncryptedCredentialsBundle\Entity\SessionRelation"
            table="encrypted_credentials_sessions">
        <id name="id" type="integer">
            <generator strategy="AUTO"/>
        </id>
        <field name="providerKey" column="provider_key" type="string" length="255"/>
        <field name="providerSessionId" column="session_id" type="string" length="255"/>
        <field name="credentialsId" column="credentials_id" type="integer"/>
    </entity>
</doctrine-mapping>
